<% content_for :title, 'Running tasks' %>
<h1>Running tasks</h1>

<table id="artists" class="table" style="width:100%" >
  <thead>
    <th>Task</th>
    <th>Status</th>
    <th>Started</th>
  </thead>

  <tbody>
    <% @tasks.each do |t| %>
      <tr>
        <td><%= t.name %></td>
        <td><%= t.status %></td>
        <td><%= t.created_at %></td>
      </tr>
    <% end %>
  </tbody>

</table>
